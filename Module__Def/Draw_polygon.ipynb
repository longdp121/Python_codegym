   "source": [
    "import turtle\n",
    "p = turtle.Turtle()\n",
    "def draw_polygon(a = int(input(\"Enter number of edges: \")), b = int(input(\"Enter edge lenght: \"))):\n",
    "    ang = ((a - 2) * 180) / a\n",
    "    for i in range(a):\n",
    "        p.fd(b)\n",
    "        p.left(180 - ang)\n",
    "    turtle.done()\n",
    "\n",
    "draw_polygon()"
   ]
  }
